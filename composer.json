{
  "name": "zrnik/cnb-exchange",
  "description": "An exchange class for 'moneyphp/money' using czech national bank exchange rates.",
  "version": "v1.2.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Štěpán Zrník",
      "email": "stepan@zrnik.eu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zrnik\\Exchange\\": "src",
      "Tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "require": {
    "PHP": ">=7.4|>=8.0",
    "ext-intl": "*",
    "ext-curl": "*",
    "psr/simple-cache": "^1",
    "psr/http-client": "^1",
    "psr/http-factory": "^1",
    "moneyphp/money": "^3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.4",
    "phpstan/phpstan": "^1",
    "tracy/tracy": "^2",
    "php-http/guzzle7-adapter": "^1",
    "http-interop/http-factory-guzzle": "^1"
  },
  "scripts": {
    "phpunit": "phpunit",
    "phpstan": "phpstan analyse --memory-limit 1G"
  }
}
